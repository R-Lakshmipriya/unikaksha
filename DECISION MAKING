#Budget
a=int(input())
b=int(input())
c=int(input())
d=int(input())
e=int(input())
p=a*350.34
q=b*230.90
r=c*190.55
t=d*125.30
s=e*180.90
x=p+q+r+t+s
if(x<=15000):
    print("yes")
else:
    print("no")


#Sece Dining
a=(input())
b=int(input())
if(a=="front" and b==1 or a=="rear" and b==2):
    print("Left Handed")
elif(a=="front" and b==2 or a=="rear" and b==1):
    print("Right Handed")
else:
    print("Invalid Input")
    

#Intersection of a Circle
import math
a=int(input())
b=int(input())
c=int(input())
a2=int(input())
b2=int(input())
c2=int(input())
d=math.sqrt((a-a2)**2+(b-b2)**2)
if(d==c+c2):
    print("The circles are tangential")
elif(d<=c+c2):
     print("The circles overlap")
else:
    print("The circles do not overlap")
    
    
#Microwave Oven
print("Enter the number of items")
a=int(input())
print("Enter the single item heating time")
b=float(input())
c=b+(50/100)*b
e=b+2*b
if(a<=2):
    print("The recommended heating time is %.2f" %c)
elif(a==3):
    print("The recommened heating time is %.2f" %e)
else:
    print("Number of items is more")



    
    
    
        
